 <!--About Banner section-->
<div class="banner-area service-banner">
    <div class="container">
        <div class="row">
            <div class="main-banner text-center">
                <h2 class="text-white">Contact Us</h2>
                <ol class="breadcrumb">
                    <li><a href="index1.html">Home</a></li>
                    <li class="active"><a href="contactus.html">Contact</a></li>
                </ol>
            </div>
        </div><!-- End off row -->
    </div><!-- End off container -->
</div><!-- End off banner section -->
<!--Newest Blog post section-->
<section id="blog-page" class="blog-page sections">
    <div class="container">
        <div class="row">
            <div class="col-md-7">
                <div class="blog-post-form-area bg-grey img-rounded">
                    <div class="blog-post-form-title">
                        <h3 class="text-black">Get In Touch</h3>
                        <div class="separator"><span></span><span></span><span></span></div>
                    </div>
                    <form class="m-top-40">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Your Name *">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Your Email *">
                                </div>
                            </div>
                            <div class="col-sm-6">
                                <div class="form-group">
                                    <input type="email" class="form-control" placeholder="Your Website (Optional)">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <textarea class="form-control textarea" rows="3" placeholder="Your Comment"></textarea>
                                </div>
                            </div>
                        </div>
                        <button type="submit" class="btn btn-primary btn-round">Submit</button>
                    </form>
                </div><!-- End off form -->
                <!-- map section-->
                <div id="map" class="map m-top-50">
                    <div class="ourmap"></div>
                </div><!-- End off Map section-->
            </div><!-- End off col-md-7 -->

            <div class="col-md-4 col-md-offset-1">
                <div class="sidebar sm-m-top-80">
                    <div class="socail-bookmark">
                        <div class="col-md-12">
                            <div class="title">
                                <h4 class="text-black">Socails</h4>
                                <div class="separator"><span></span><span></span><span></span></div>
                            </div>
                            <div class="contact-boxv1-item m-top-30">
                                <ul class="list-inline">
                                    <li><a href=""><i class="icon icon-social-2"></i></a></li>
                                    <li><a href=""><i class="icon icon-social-1"></i></a></li>
                                    <li><a href=""><i class="icon icon-social-network"></i></a></li>
                                    <li><a href=""><i class="icon icon-social"></i></a></li>
                                </ul>
                            </div>
                        </div><!-- End off col-md-4 -->
                        <div class="col-md-12 m-top-80">
                            <div class="title">
                                <h4 class="text-black">Address</h4>
                                <div class="separator"><span></span><span></span><span></span></div>
                            </div>
                            <div class="contact-boxv1-item m-top-30">
                                <div class="boxv1-item-img">
                                    <img src="~/assets/images/location-img.png" alt="">
                                </div>
                                <div class="boxv1-item-content text-black">
                                    <p class="m-top-10 text-black">
                                        1877 Perry Street Swartz Creekson,
                                        MI 48473
                                    </p>
                                </div>
                            </div>
                        </div><!-- End off col-md-4 -->
                        <div class="col-md-12 m-top-80">
                            <div class="title">
                                <h4 class="text-black">Our Phone &amp; Fax</h4>
                                <div class="separator"><span></span><span></span><span></span></div>
                            </div>
                            <div class="contact-boxv1-item m-top-30">
                                <div class="boxv1-item-img">
                                    <img src="~/assets/images/phone-fax-img.png" alt="">
                                </div>
                                <div class="boxv1-item-content text-black">
                                    <p class="m-top-10">
                                        Phone:  (012) 345 - 6789 <br>
                                        Fax:  555 - 675 0037
                                    </p>
                                </div>
                            </div>
                        </div><!-- End off col-md-4 -->
                        <div class="col-md-12 m-top-80">
                            <div class="title">
                                <h4 class="text-black">Email</h4>
                                <div class="separator"><span></span><span></span><span></span></div>
                            </div>
                            <div class="contact-boxv1-item m-top-30">
                                <div class="boxv1-item-img">
                                    <img src="~/assets/images/envelop-img-02.png" alt="">
                                </div>
                                <div class="boxv1-item-content text-black">
                                    <p class="m-top-10">
                                        Info:  seo@info.com <br>
                                        Salse:  seo@salse.com
                                    </p>
                                </div>
                            </div>
                        </div><!-- End off col-md-4 -->
                        <div class="col-md-12 m-top-80">
                            <div class="title">
                                <h4 class="text-black">Working Hours</h4>
                                <div class="separator"><span></span><span></span><span></span></div>
                            </div>
                            <div class="contact-boxv1-item m-top-30">
                                <div class="boxv1-item-img">
                                    <img src="~/assets/images/clock-img.png" alt="">
                                </div>
                                <div class="boxv1-item-content text-black">
                                    <p class="">
                                        Monday to Saturday: 8:00am - 9:00pm
                                        Sunday: Closed
                                    </p>
                                </div>
                            </div>
                        </div><!-- End off col-md-4 -->
                    </div>

                </div>
            </div><!-- End off col-md-4 -->
        </div> <!-- End off row -->
    </div> <!-- End off container -->
</section> <!-- End off Blog page section-->
