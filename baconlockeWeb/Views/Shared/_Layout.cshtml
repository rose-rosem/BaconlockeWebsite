<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title - 我的 ASP.NET 应用程序</title>
    <meta name="description" content="">
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    @Styles.Render("~/layout/css")
    @Scripts.Render("~/layout/modernizr")

</head>
<body data-spy="scroll" data-target=".navbar-collapse">

    <!-- Preloader -->
    <div class="preloader loaderbox">
        <div id="cupcake" class="loaderbox">
            <span class="letter">L</span>
            <div class="cupcakeCircle loaderbox"><div class="cupcakeInner loaderbox"><div class="cupcakeCore loaderbox"></div></div></div>
            <span class="letter loaderbox">A</span><span class="letter loaderbox">D</span><span class="letter loaderbox">I</span><span class="letter loaderbox">N</span><span class="letter loaderbox">G</span>
        </div>
    </div>
    <!--End off Preloader -->
    <!--Home page style-->
    <nav class="navbar home1_menu navbar-fixed navbar-default navbar-mobile bootsnav is-visible xsaa" data-nav-status="toggle">
        <div class="container-fluid">
            <div class="row">
                <div class="container">
                    <div class="row">
                        <!-- Start Header Navigation -->
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-menu">
                                <i class="fa fa-bars"></i>
                            </button>
                            <a class="navbar-brand" href="/"><img src="~/assets/images/logov1.png" class="logo" alt=""></a>
                        </div>
                        <!-- End Header Navigation -->
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="navbar-menu">
                            <ul class="nav navbar-nav navbar-center" data-in="fadeIn" data-out="fadeOut">
                                <li><a href="/">首页</a></li>
                                <li><a href="/Home/Advantage">培洛优势</a></li>
                                <li><a href="/Home/Course">培洛课程</a></li>
                                <li><a href="/Home/StudentCase">学员案例</a></li>
                                <li><a href="/Home/Download">下载中心</a></li>
                                <li><a href="/Home/HelpFile">帮助文档</a></li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">关于我们</a>
                                    <ul class="dropdown-menu">
                                        <li><a href="/Home/About">公司简介</a></li>
                                        <li><a href="/Home/Contact">联系我们</a></li>
                                        <li><a href="/Home/Methodogy">培洛方法论</a></li>
                                    </ul>
                                </li>
                                <li><a style="font-size:24px;color:#13A0B2;font-weight:bold;">400-878-4867</a></li>
                            </ul>
                        </div><!-- /.navbar-collapse -->
                    </div>
                </div>
            </div>
        </div>
    </nav>
    <!-- End off Nav -->

    <div class="culmn">

        @RenderBody()


        <!-- Footer Widget section -->
        <section id="contact" class="contact clear">
            <div class="container">
                <div class="row">
                    <div class="main-contact-boxv1">
                        <div class="col-md-4">
                            <div class="contact-boxv1-item">
                                <div class="boxv1-item-img">
                                    <img src="~/assets/images/location-img.png" alt="" />
                                </div>
                                <div class="boxv1-item-content text-black">
                                    <h4>我们的地址</h4>
                                    <p class="m-top-10">无锡惠山区钱桥育才路27号</p>
                                </div>
                            </div>
                        </div><!-- End off col-md-4 -->
                        <div class="col-md-4">
                            <div class="contact-boxv1-item sm-m-top-30">
                                <div class="boxv1-item-img">
                                    <img src="~/assets/images/phone-fax-img.png" alt="" />
                                </div>
                                <div class="boxv1-item-content text-black">
                                    <h4>我们热线</h4>
                                    <p class="m-top-10">400-878-4867 / 0510-83480622</p>
                                </div>
                            </div>
                        </div><!-- End off col-md-4 -->
                        <div class="col-md-4">
                            <div class="contact-boxv1-item sm-m-top-30">
                                <div class="boxv1-item-img">
                                    <img src="~/assets/images/clock-img.png" alt="" />
                                </div>
                                <div class="boxv1-item-content text-black">
                                    <h4>我们的邮箱</h4>
                                    <p class="">baconlocke@163.com</p>
                                </div>
                            </div>
                        </div><!-- End off col-md-4 -->
                    </div>
                </div>
            </div>
            <!-- End off container -->

        </section>
        <!-- End off footer widget Section -->
        <!--Footer section-->
        <footer class="footerv1 bg-black">
            <div class="scrollup back">
                <a href="#"><img class="hvr-hang" src="~/assets/images/go-top-img.png" alt="返回顶部" /></a>
            </div>
            <div class="wechatdiv">
                <div class="wechat scrollup">
                    <img class="hvr-hang" src="~/assets/images/wechat.png" alt="微信" />
                </div>
                <img class="wechatimg hvr-hang" src="~/assets/images/weixin.png" alt="微信号" />
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="main-footer wow fadeInUp">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="copyright-text text-center">
                                        <p class="">Copyright &copy; @DateTime.Now.Year .Company name All rights reserved.</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <!-- End off footer Section-->
    </div>

    <!-- JS includes -->
    @Scripts.Render("~/layout/js")
    @RenderSection("scripts", required: false)
</body>
</html>
